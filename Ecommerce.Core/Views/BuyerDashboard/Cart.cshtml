@{
    Layout = "~/Views/Shared/_dashboardSeller.cshtml";
}


<div class="d-flex container justify-content-center align-items-center flex-column mt-3">
    <span class="fs-2 text-color"> My Cart</span>
</div>
<div class="px-2 w-100 d-flex justify-content-center align-items-center">
    <div class="container p-3 m-3 d-flex cart rounded-3 align-items-center flex-column ">
        <div id="CartContainer">
        </div>

        <div class="d-flex justify-content-center m-5 w-100">
            <span class="loader3" style="width: 40px; height: 40px;"></span>
        </div>
    </div>
</div>


@section scripts {
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script>
        window.successMessage = '@TempData["SuccessMessage"]';
        window.errorMessage = '@TempData["ErrorMessage"]';

    </script>
    <script src="~/js/toastr_script.js"></script>
    <script src="~/js/cart_script.js"></script>
}